{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.framework.id": "hugo",
  "frontMatter.content.publicFolder": "static",
  "frontMatter.content.pageFolders": [
    {
      "title": "books",
      "path": "[[workspace]]/hugo/content/books"
    },
    {
      "title": "posts",
      "path": "[[workspace]]/hugo/content/posts"
    },
    {
      "title": "Writings",
      "path": "[[workspace]]/hugo/content/writings"
    },
    {
      "title": "programming",
      "path": "[[workspace]]/hugo/content/programming"
    },
    {
      "title": "sidebar",
      "path": "[[workspace]]/hugo/content/sidebar"
    }
  ],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string",
          "single": true
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "lastmod",
          "name": "lastmod",
          "type": "datetime"
        },
        {
          "title": "publishdate",
          "name": "publishdate",
          "type": "datetime"
        },
        {
          "title": "draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "background",
          "name": "background",
          "type": "string"
        },
        {
          "title": "displayClass",
          "name": "displayClass",
          "default": "default",
          "type": "choice",
          "choices": [
            "default",
            "poem",
            "article",
            "multiple"
          ]
        }
      ]
    },
    {
      "name": "sidebar",
      "pageBundle": false,
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string",
          "single": true
        },
        {
          "title": "draft",
          "name": "draft",
          "type": "draft"
        }
      ]
    },
    {
      "name": "chapter",
      "pageBundle": false,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string",
          "single": true
        },
        {
          "title": "Subtitle",
          "name": "subtitle",
          "type": "string",
          "single": true
        },
        {
          "title": "Date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "Draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Universe",
          "name": "universe",
          "type": "string"
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "Chapter",
          "name": "chapter",
          "type": "number"
        },
        {
          "title": "Books",
          "name": "books",
          "type": "taxonomy",
          "taxonomyId": "booksTaxonomy"
        },
        {
          "title": "Background",
          "name": "background",
          "type": "string"
        },
        {
          "title": "Type",
          "name": "type",
          "type": "string"
        },
        {
          "title": "slug",
          "name": "slug",
          "type": "slug"
        }
      ]
    }
  ],
  "frontMatter.preview.host": "http://localhost:1313/",
